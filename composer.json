{
  "name": "monolith/configuration",
  "description": "Configuration management module for the Monolith framework.",
  "keywords": [
    "monolith",
    "http"
  ],
  "homepage": "https://www.getmonolith.com",
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawn@heybigname.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "vlucas/phpdotenv": "^2.5@dev",
    "monolith/component-bootstrapping": "^4"
  },
  "require-dev": {
    "phpspec/phpspec": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\Configuration\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\Configuration\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
