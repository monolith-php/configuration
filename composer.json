{
  "name": "monolith/configuration",
  "description": "Configuration management module for the Monolith framework.",
  "keywords": [
    "monolith",
    "http"
  ],
  "require": {
    "php": ">=8",
    "monolith/component-bootstrapping": "^8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3@dev"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\Configuration\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
